{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "allowJs": true,
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "lib": [ "es2015", "dom" ],
    "resolveJsonModule": true
  }
}